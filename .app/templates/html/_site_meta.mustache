<meta charset="utf-8">
<!-- browser/device compatibility stuff -->
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<meta name="HandheldFriendly" content="True"/>
<meta name="format-detection" content="telephone=no">
<!-- page info -->
<title>{{page_title}} | {{blog_title}}</title>
<meta name="description" content="{{page_descr}}">
<meta name="keywords" content="{{page_keywords}}">
<meta name="news_keywords" content="{{page_keywords}}">
<meta name="subject" content="{{page_keywords}}">
<meta name="generator" content="{{site_title}}">
<meta name="medium" content="blog">
<meta name="copyright" content="&copy; {{site_title}}">
<meta name="language" content="en">
<meta name="robots" content="index,follow">
<meta name="topic" content="{{page_keywords}}">
<meta name="owner" content="{{page_author}}">
<meta name="pagename" content="{{page_title}}">
<!-- Apple -->
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-title" content="{{site_title}}">
<!-- Facebook -->
<meta property="og:title" content="{{page_title}}">
<meta property="og:description" content="{{page_descr}}">
<meta property="og:url" content="https://{{blog_domain}}{{page_url}}">
<meta property="og:site_name" content="{{site_title}}">
<meta property="og:type" content="article">
<meta property="og:locale" content="en">
<meta property="og:image" content="https://{{blog_domain}}{{blog_url}}/assets/icons/rss.png">
<meta property="article:author" content="{{page_author}}">
<meta property="article:section" content="">
<meta property="article:published_time" content="{{page_created}}">
<meta property="article:modified_time" content="{{page_modified}}">
<!-- Twitter -->
<meta name="twitter:card" content="summary">
<meta name="twitter:title" content="{{page_title}}">
<meta name="twitter:site" content="{{page_twitter}}">
<meta name="twitter:creator" content="{{page_twitter}}">
<meta name="twitter:image" content="https://{{blog_domain}}{{blog_url}}/assets/icons/rss.png">
<meta name="twitter:domain" content="https://{{blog_domain}}{{blog_url}}">
<!-- Linked data -->
{{#ld_website}}
{{ld_website}}
{{/ld_website}}
{{ld_article}}
{{ld_breadcrumb}}
{{#ld_itemlist}}
{{ld_itemlist}}
{{/ld_itemlist}}
<!-- RSS feed -->
<link rel="alternate" type="application/rss+xml" title="Subscribe to {{site_title}}" href="{{blog_url}}/feed.rss">
<!-- SEO stuff -->
<link rel="author" href="/humans.txt">
<link rel="canonical" href="//{{blog_domain}}{{page_url}}">
<!-- App icons and favicons -->
<link rel="manifest" href="{{blog_url}}/manifest.webmanifest?v={{timestamp}}">
<link rel="icon" type="image/png" sizes="16x16" href="{{blog_url}}/assets/icons/favicon-16x16.png?v={{timestamp}}">
<link rel="icon" type="image/png" sizes="32x32" href="{{blog_url}}/assets/icons/favicon-32x32.png?v={{timestamp}}">
<link rel="icon" type="image/png" sizes="192x192" href="{{blog_url}}/assets/icons/android-chrome-192x192.png?v={{timestamp}}">
<link rel="icon" type="image/vnd.microsoft.icon" href="{{blog_url}}/assets/icons/favicon.ico?v={{timestamp}}">
<link rel="apple-touch-icon" sizes="180x180" href="{{blog_url}}/assets/icons/apple-touch-icon.png?v={{timestamp}}">
<link rel="shortcut icon" href="{{blog_url}}/assets/icons/favicon.ico?v={{timestamp}}">
<link rel="mask-icon" color="#444444" href="{{blog_url}}/assets/icons/safari-pinned-tab.svg?v={{timestamp}}">
<meta name="msapplication-TileColor" content="#ffffff">
<meta name="msapplication-config" content="{{blog_url}}/assets/icons/browserconfig.xml?v={{timestamp}}">
<meta name="theme-color" content="#101010">
<!-- Fallback app name, for legacy browsers -->
<meta name="application-name" content="{{site_title}}">
<!-- prefetch resources -->
<link rel="dns-prefetch" href="//unpkg.com">
<link rel="preconnect"   href="//unpkg.com" crossorigin>
<!-- site css -->
<style>{{inline_css}}</style>
<link rel="stylesheet" href="{{blog_url}}/assets/css/main.css?v={{timestamp}}">
{{#page_specific_css_file}}
<!-- page specific css -->
<link rel="stylesheet" href="{{page_specific_css_file}}">
{{/page_specific_css_file}}
{{#pygments}}
<!-- pygments (syntax highlighting) -->
<link rel="stylesheet" href="{{blog_url}}/assets/css/pygments-{{pygments_theme}}.css?v={{timestamp}}">
{{/pygments}}
{{#page_google_analytics_js}}
<!-- google analytics -->
<script id="google-analytics">
 var _gaq = _gaq || [];
 _gaq.push(["_setAccount", "{{page_google_analytics_id}}"]);
 _gaq.push(["_trackPageview"]);
 (function(){
  var ga = document.createElement("script"); ga.type = "text/javascript"; ga.async = true;
  ga.src = ("https:" == document.location.protocol ? "https://ssl" : "http://www") + ".google-analytics.com/ga.js";
  var s = document.getElementsByTagName("script")[0]; s.parentNode.insertBefore(ga, s);
 })();
</script>
{{/page_google_analytics_js}}
<script>
// enable styling of some HTML elements in IE < 9
document.createElement("picture");
document.createElement("time");
</script>
{{#page_js_deps}}
<script class="unpkg" defer src="//unpkg.com/{{.}}"></script>
{{/page_js_deps}}
{{#site_js}}
<!-- main javascript app -->
<script id="appjs" defer src="{{blog_url}}/assets/js/app.js?v={{timestamp}}"></script>
{{/site_js}}
